{
  "name": "@adam-26/jsen",
  "description": "JSON-Schema validator built for speed",
  "author": "Veli Pehlivanov <bugventure@gmail.com>",
  "homepage": "https://github.com/adam-26/jsen",
  "repository": {
    "type": "git",
    "url": "https://github.com/adam-26/jsen.git"
  },
  "bugs": "https://github.com/adam-26/jsen/issues",
  "keywords": [
    "JSON",
    "schema",
    "validator",
    "speed"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "build": "rm -rf dist && pwd && ls -a && mkdir dist && cat ./lib/header > ./dist/jsen.js && ./node_modules/.bin/browserify index.js -s jsen >> ./dist/jsen.js && ./node_modules/.bin/uglify -s ./dist/jsen.js -o ./dist/jsen.min.temp && cp ./lib/header ./dist/jsen.min.js && cat ./dist/jsen.min.temp >> ./dist/jsen.min.js && rm ./dist/jsen.min.temp",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls && rm -rf ./coverage",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prepublish": "cd $TRAVIS_BUILD_DIR && npm run build"
  },
  "devDependencies": {
    "browserify": "10.x",
    "coveralls": "2.x",
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-uglify": "^1.0.1",
    "istanbul": "0.x",
    "json-schema-test-suite": "0.0.10",
    "mocha": "1.x",
    "mocha-lcov-reporter": "^1.2.0",
    "semantic-release": "^4.3.5",
    "uglify": "0.x"
  },
  "release": {
    "branch": "fork"
  }
}
